{
  "name": "pw3.0",
  "version": "1.0.0",
  "description": "This is a web application built by webpack2.0",
  "scripts": {
    "build-dll": "./node_modules/.bin/webpack --progress --colors --config ./webpack-dll.config.js",
    "dll": "npm run clean-dll && npm run build-dll",
    "build-webpack": "./node_modules/.bin/webpack --progress --colors --bail",
    "build": "npm run clean-build && npm run build-webpack",
    "dev-webpack": "./node_modules/.bin/webpack --progress --colors --config ./webpack.dev.config.js",
    "dev": "npm run clean-build && npm run dev-webpack",
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack.dev.config.js --open --hot",
    "clean-dll": "node ./npm-scripts/before-build-dll.script.js",
    "clean-build": "node ./npm-scripts/before-build.script.js",
    "clean": "npm run clean-dll && npm run clean-build",
    "profile": "./node_modules/.bin/webpack --colors --profile --display-modules --config ./webpack.dev.config.js"
  },
  "author": "Super Liu",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^2.1.4",
    "jquery-mockjax": "^2.2.1"
  },
  "devDependencies": {
    "toastr":"^2.1.2",
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.26.0",
    "ejs-loader": "^0.3.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.3.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-validator": "^2.2.7"
  }
}
